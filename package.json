{
	"name": "anvisa-bula-wrapper",
	"version": "1.0.0",
	"description": "API wrapper para a busca da anvisa",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"test": "test",
		"start": "npx cross-env NODE_ENV=development nodemon index.mjs",
		"start:prod": "npx cross-env NODE_ENV=production node index.mjs",
		"type:generate": "npx -p typescript tsc ./src/**/*.mjs  --declaration --allowJs --emitDeclarationOnly --skipLibCheck --declarationMap --outFile  ./types/index.d.ts"
	},
	"keywords": [
		"bula",
		"Anvisa",
		"api"
	],
	"types": "types/index.d.ts",
	"author": "Victor Raton",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^16.0.1",
		"puppeteer": "^15.3.2",
		"redis": "^4.2.0"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"eslint": "^8.19.0",
		"nodemon": "^2.0.19",
		"puppeteer-core": "^15.3.2"
	}
}
